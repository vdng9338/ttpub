}

.source {
	border: 1px solid #999;
	overflow: auto;
	background-color: #eee;
}

dt {
	padding: 4px 4px 4px 24px;
	color: #333333;
	background-color: #ccc;
	font-weight: bold;
	font-size: 14px;
}

